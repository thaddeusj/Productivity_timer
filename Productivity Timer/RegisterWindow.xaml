<Window x:Class="Productivity_Timer.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RegisterWindow" Height="151" Width="397">
    <Grid>
        <TextBox x:Name="UsernameBox" HorizontalAlignment="Left" Height="25" Margin="111,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="268"/>
        <TextBox x:Name="PasswordBox" HorizontalAlignment="Left" Height="25" Margin="111,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="268"/>
        <Label Content="Username" HorizontalAlignment="Left" Height="25" Margin="10,10,0,0" VerticalAlignment="Top" Width="96"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="25" Margin="10,40,0,0" VerticalAlignment="Top" Width="96"/>
        <Button x:Name="RegisterButton" Content="Register" HorizontalAlignment="Left" Height="25" Margin="264,86,0,0" VerticalAlignment="Top" Width="55" Click="RegisterButton_Click"/>
        <Button x:Name="CancelButton" Content="Cancel" HorizontalAlignment="Left" Height="25" Margin="324,86,0,0" VerticalAlignment="Top" Width="55" Click="CancelButton_Click"/>
    </Grid>
</Window>
